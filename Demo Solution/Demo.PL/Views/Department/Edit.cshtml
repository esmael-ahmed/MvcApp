@model DepartmentViewModel
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit @Model.Name Department</h1>
<div asp-validation-summary="All"></div>

<form class="mt-3" asp-action="Edit">
    <partial name="PartialViews/_EditCreatePartialView" />
    <input type="submit" value="Save" asp-route-id="@Model.Id" class="btn btn-primary" />  |
    <a asp-action="Index" class="btn btn-light">Back To List</a>
</form>
@section ValidationScriptSection {
    <partial name="_ValidationScripts" />
}